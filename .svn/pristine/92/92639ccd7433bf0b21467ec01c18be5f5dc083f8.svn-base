<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#80000000"
    tools:context=".SelectWorld" >

    <Button
        android:id="@+id/butnClose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/cerrar"
        android:contentDescription="@string/frikiadosImage"
        android:visibility="visible" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:background="@color/red" >

        <ImageView
            android:id="@+id/imgRayo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:background="@drawable/rayo_1"
            android:contentDescription="@+id/frikiadosImage"
            android:gravity="center"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/logoJefe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:background="@drawable/logo_jefe"
            android:contentDescription="@+id/frikiadosImage"
            android:gravity="center"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/imgHumoOne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/logoJefe"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:background="@drawable/humo_1"
            android:contentDescription="@+id/frikiadosImage"
            android:gravity="center"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/imgJefe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/logoJefe"
            android:layout_centerHorizontal="true"
            android:background="@drawable/jefe"
            android:contentDescription="@+id/frikiadosImage"
            android:gravity="center"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/imgHumoTwo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/logoJefe"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="80dp"
            android:background="@drawable/humo_2"
            android:contentDescription="@+id/frikiadosImage"
            android:gravity="center"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textJefeComic"
            style="@style/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:layout_below="@+id/imgHumoTwo"
            android:text="@string/frikiados_boss_manga" />
    </RelativeLayout>

</RelativeLayout>